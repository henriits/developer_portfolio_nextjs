datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  githubLink  String?
  liveLink    String?
  imageUrl    String?  // Optional field for storing the image URL
  technologies String[] // Array of strings to store technologies
}